<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="anchorPaneE1" prefHeight="569.0" prefWidth="695.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.controllers.Frontpartenaire">
   <children>
      <VBox spacing="10">
         <children>
            <!-- Button and TextField section -->
            <HBox alignment="CENTER" prefHeight="54.0" prefWidth="695.0" spacing="10">
               <children>

                  <!-- Search TextField -->
                  <TextField fx:id="filtre" onKeyPressed="#handleSearch" prefHeight="43.0" prefWidth="375.0" promptText="Search.................">
                     <font>
                        <Font size="13.0" />
                     </font></TextField>


                  <!-- Back Button -->
                  <Button onAction="#Back" prefHeight="35.0" prefWidth="106.0" style="-fx-background-color: skyblue;" text="Back">
                     <font>
                        <Font name="System Bold Italic" size="14.0" />
                     </font></Button>
               </children>

               <!-- You can add more nodes as needed -->

            </HBox>

            <!-- ScrollPane section -->
            <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="529.0" prefWidth="695.0">
               <content>
                  <ListView fx:id="partenaireview" layoutX="10.0" layoutY="50.0" prefHeight="300.0" prefWidth="400.0">

                  </ListView>
               </content>
            </ScrollPane>
         </children>
      </VBox>

   </children>
</AnchorPane>
